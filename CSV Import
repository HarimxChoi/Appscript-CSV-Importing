function CSVimport() {

  var Folder = DriveApp.getFolderById("folder id")
  
  var file = Folder.getFilesByName("file name").next();
  
  / Select Characteristics Encoder ex. "EUC-KR", "UTF-8", ...
  
  var csvData = Utilities.parseCsv(file.getBlob().getDataAsString("EUC-KR"));
  
  var sheet = SpreadsheetApp.getActive().getSheetByName('sheet name');
  
  sheet.getRange(1, 1, csvData.length, csvData[0].length).setValues(csvData);
  
}
