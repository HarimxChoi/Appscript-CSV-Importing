function RPARESULTBACKUP() {

var formattedDate = Utilities.formatDate(new Date(), "GMT+9", "yyyy-MM-dd' 'HH:mm");
var name = SpreadsheetApp.openById("160tYjin9udG7c_kmHngJl2tbNZ3hVY4JzlgpI4ICHRU").getName() + " Backup " + formattedDate;
var destination = DriveApp.getFolderById("1hcwInMZLakJMJHhp5-cs7C6nUiLnHimj");
var file = DriveApp.getFileById("160tYjin9udG7c_kmHngJl2tbNZ3hVY4JzlgpI4ICHRU")

file.makeCopy(name, destination);
}



